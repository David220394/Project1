<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pnlPatient" style="-fx-background-color: #02030A;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.management.controller.PatientController">
   <children>
      <ScrollPane layoutY="65.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
         <content>
            <JFXTreeTableView fx:id="patientTableView" prefWidth="671.0" style="-fx-background-color: #02030A;" />
         </content>
      </ScrollPane>
      <Label text="Patient" textFill="#e7e5e5" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <TextField fx:id="search" prefHeight="33.0" prefWidth="198.0" promptText="Search" style="-fx-background-color: #02030A; -fx-border-color: #B7C3D7; -fx-border-radius: 2em;" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <JFXButton onAction="#addPatient" text="ADD" textFill="WHITE" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="70.0" />
   </children>
</AnchorPane>
